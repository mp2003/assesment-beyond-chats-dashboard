	"scripts": {
		"start": "dotenv -e .env.production react-scripts start",
		"build": "cross-env GENERATE_SOURCEMAP=false dotenv -e .env.production react-scripts build",
		"build:push": "npm run build && dotenv -e .env.production node netlifyDeploy.js",
		"start:dev": "dotenv -e .env.development react-scripts start",
		"build:dev": "dotenv -e .env.development react-scripts build",
		"build:push:dev": "npm run build:dev && dotenv -e .env.development node netlifyDeploy.js",
		"start:test": "dotenv -e .env.test react-scripts start",
		"build:test": "GENERATE_SOURCEMAP=false dotenv -e .env.test react-scripts build",
		"build:push:test": "npm run build:test && dotenv -e .env.test node netlifyDeploy.js",
		"format": "npx prettier --write .",
		"buildWindows": "set 'GENERATE_SOURCEMAP=false' && react-scripts build",
		"test": "react-scripts test",
		"coverage": "npm test --env=jsdom --coverage --watchAll --collectCoverageFrom='src/components/**/*.{js,jsx}' --collectCoverageFrom='!src/components/experiment/**'",
		"eject": "react-scripts eject",
		"server": "npm run build && node server/index.js",
		"serverWindows": "npm run buildWindows && node server/index.js"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@types/react-router-dom": "^5.3.3",
		"@types/sortablejs": "^1.13.0",
		"archiver": "^6.0.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"dotenv-cli": "^7.4.1",
		"eslint-config-react-app": "^7.0.1",
		"eslint-plugin-mui-unused-classes": "^2.0.2",
		"prettier": "3.1.1"
	}
}
